generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  username      String?
  password      String?
  role      String?
  createdAt DateTime @default(now())
}
 
model products {
  id        Int      @id @default(autoincrement())
  productName     String?
  productPrice      Int?
  productDescription      String?
  productCategory      String?
  productUrlImg      String?
  createdAt DateTime @default(now())
}